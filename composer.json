{
  "name": "louisk/laravel-push-fcm",
  "description": "Push Notification Package for Laravel",
  "keywords": [
    "push",
    "notification",
    "php",
    "laravel"
  ],
  "homepage": "https://github.com/keviinlouis/LaravelPushFCM",
  "support": {
    "issues": "https://github.com/keviinlouis/LaravelPushFCM/issues",
    "source": "https://github.com/keviinlouis/LaravelPushFCM"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Kevin Louis",
      "email": "keviinlouiis@gmail.com",
      "homepage": "https://github.com/keviinlouis",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.0",
    "illuminate/support": "5.1.* || 5.2.* || 5.3.* || 5.4.* || 5.5.* || 5.6.* || 5.7.*",
    "brozot/laravel-fcm": "^1.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8 || ~6.0"
  },
  "autoload": {
    "psr-4": {
      "Louis\\LaravelPushFCM\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Louis\\LaravelPushFCM\\Test\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-develop": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "Louis\\LaravelPushFCM\\PushNotificationServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  }
}
